#include <stdlib.h>
#include <stdio.h>

int *rd(int *v, int n)
{
	int i,j,k=0,*r;
	
	r = malloc(sizeof(int));
	
	for(i=0; i<n; i++)
	{
		for(j=0; j<k; j++)
		{
			if(v[i] == r[j])
				break;
		}
		
		if(j == k)
		{
					
			r[k] = v[i];
			k++;
			
		r = realloc(r,(k+1)*sizeof(int));	
					
		}
		
		
			
	}

return r;

}

int main()
{

int n,i;

printf("Digite o tamnho do vetor:\n");
scanf("%d", &n);

int vet[n];
int *saida;

for(i=0;i<n;i++)
	scanf("%d",&vet[i]);

saida = rd(vet, n);
	
	for(i=0; i<sizeof(saida)/sizeof(int); i++)
		printf("%d ", saida[i]);




return 0;

}
