#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#define Tint sizeof(int)

void *B = NULL;

int *i = NULL;

int *j = NULL;

int *aux = NULL;

void bs(int *pv[], int *aux);



int main(){

B = malloc(11*Tint);

i = B + 7*Tint;

j = B + 8*Tint;

int *aux = B + 9*Tint;

int *Pint = B;

int *pv[7];

int Vet[] = {9,5,21,6,7,1,0};

for(int i=0; i<7; i++){
	*Pint = Vet[i];
	Pint++;}

Pint = B;


for(int i=0; i<7; i++){
	printf("%d ", *Pint);
	Pint++;}

Pint = B;

for(int i=0; i<7; i++){
	*pv[i] = *Pint; 
	Pint++;}
	
bs(pv,aux);

Pint = B;

for(int i=0; i<7; i++){
	*Pint = *pv[i];
	Pint++;}


printf("\n");

Pint = B;

for(int i=0; i<7; i++){
	printf("%d ", *Pint);
	Pint++;}
	

return 0;
}

void bs(int *pv[], int *aux){
		
	for(*i=6;*i>0;(*i)--){
		for(*j=0;*j<*i;(*j)++){
			if(pv[*j] > pv[*j+1]){
				*aux = *pv[*j];
				*pv[*j] = *pv[*j+1];
				*pv[*j+1] = *aux;}
			}
		}
	} 
				
		
	
