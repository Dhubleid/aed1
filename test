#include <stdio.h>
#include <string.h>
#include <stdlib.h>

char** separa(char* l)
{
    int i = 0;
    char* s;
    char** a = calloc(41, sizeof(char*));

    if(!a){
    exit(1);
    }

    printf("abc|");

   while((s = strtok(l," ")) !=  NULL){

        a[i] = s;

   }

    return a;



}

int main()
{
    char* linha;
    char** saida;

    printf("digite a linha:\n");

    linha = malloc(81*sizeof(char));

    fgets(linha,81,stdin);

    printf("ghv");

    saida = separa(linha);

    for(int i=0; saida[i]; i++)
        printf("%s\n", saida[i]);


    return 0;

}
