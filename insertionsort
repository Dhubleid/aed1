#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void insertion_sort(int vet[],int n);
void insertion_sortOA(char nomes[][20],int k);

int main(){

int n,i;

printf("\nDigite N\n");
scanf("%d",&n);

int vet[n];

printf("\nDigite os numeros:\n");

for(i=0;i<n;i++){

    scanf("%d",&vet[i]);

    }

printf("\n");

for(int i=0;i<n;i++){

    printf(" %d ",vet[i]);

}


insertion_sort(vet,n);

printf("\n\nNumeros Ordenados\n");

for(int i=0;i<n;i++){

    printf(" %d ",vet[i]);

}

printf("\nDigite a quantidade de strings:\n");

int k;

scanf("%d",&k);

char nomes[k][20];

getchar();

printf("\nDigite os nomes:");

for(i=0;i<k;i++){
    fgets(nomes[i],20,stdin);
}

printf("\nNomes:");

for(i=0;i<k;i++){
    printf("\n%s",nomes[i]);
}

insertion_sortOA(nomes,k);

printf("\nNomes Ordenados: \n");

for(i=0;i<k;i++){
    printf("\n%s",nomes[i]);
}


return 0;


}
void insertion_sort(int vet[],int n){
    int i, j, aux;

    for(i=1;i<n;i++){
      aux=vet[i];
      j=i-1;

      while(j>=0 && vet[j]>aux){
        vet[j+1]=vet[j];
        j--;}

      vet[j+1]=aux;
      }
}

void insertion_sortOA(char nomes[][20],int k){
    int i, j;
    char aux[20];

    for(i=1;i<n;i++){
      strcpy(aux,nomes[i]);
      j=i-1;

      while(j>=0 && (strcmp(nomes(j),aux)>0){
        strcpy(nomes[j+1],nomes[j]);
        j--;}

      strcpy(nomes[j+1],aux);}
  }





